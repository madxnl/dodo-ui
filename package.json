{
  "name": "@madxnl/dodo-ui",
  "version": "0.40.12",
  "license": "ISC",
  "description": "Dodo UI Framework for Vue.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madxnl/dodo.git"
  },
  "author": "Doeke de Wolf",
  "bugs": {
    "url": "https://github.com/madxnl/dodo/issues"
  },
  "homepage": "https://github.com/madxnl/dodo#readme",
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/dodo-ui.js",
  "main": "./dist/dodo-ui.umd.cjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/dodo-ui.js",
      "require": "./dist/dodo-ui.umd.cjs"
    },
    "./dist/*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "vite --debug hmr",
    "build:docs": "vite build",
    "build:npm": "vite build --config vite.npm.ts && vue-tsc --emitDeclarationOnly -p tsconfig.npm.json",
    "preview": "vite preview",
    "build": "npm run build:npm && npm run build:docs",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --max-warnings=0",
    "type-check": "vue-tsc --build --force",
    "check": "npm run lint && npm run type-check",
    "prepublishOnly": "npm i && npm run check && npm run build",
    "format": "prettier --write src/"
  },
  "devDependencies": {
    "@material-design-icons/font": "^0.14.13",
    "@playwright/test": "^1.46.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.3.0",
    "@types/prismjs": "^1.26.4",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "c8": "^10.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.27.0",
    "jsdom": "^24.1.1",
    "material-symbols": "^0.22.0",
    "npm-run-all2": "^6.2.2",
    "prettier": "^3.3.3",
    "prismjs": "^1.29.0",
    "sirv": "^2.0.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.1.0",
    "vite": "^5.4.1",
    "vite-plugin-vue-devtools": "^7.3.8",
    "vitest": "^2.0.5",
    "vue": "^3.4.38",
    "vue-docgen-api": "^4.79.2",
    "vue-router": "^4.4.3",
    "vue-tsc": "^2.0.29"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "vue-router": {
      "optional": true
    }
  }
}
