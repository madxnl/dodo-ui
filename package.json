{
  "name": "@madxnl/dodo-ui",
  "version": "0.30.1",
  "license": "ISC",
  "description": "Dodo UI Framework for Vue.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madxnl/dodo.git"
  },
  "author": "Doeke de Wolf",
  "bugs": {
    "url": "https://github.com/madxnl/dodo/issues"
  },
  "homepage": "https://github.com/madxnl/dodo#readme",
  "files": [
    "dist"
  ],
  "main": "./dist/dodo-ui.umd.js",
  "module": "./dist/dodo-ui.es.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "check:test": "vitest run",
    "check:lint": "eslint . --ext .js,.ts,.vue --max-warnings=0",
    "check:ts": "vue-tsc --noEmit",
    "check": "npm run check:lint && npm run check:test && npm run check:ts",
    "build:docs": "vite build",
    "build:npm": "vite build --config vite.npm.ts && vue-tsc --emitDeclarationOnly -p tsconfig.npm.json",
    "build": "npm run build:npm && npm run build:docs",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@material-design-icons/font": "^0.14.9",
    "@types/node": "20.4.2",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.0",
    "c8": "^8.0.0",
    "eslint": "^8.45.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.15.1",
    "jsdom": "^22.1.0",
    "prismjs": "^1.29.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "vite-plugin-inspect": "^0.7.33",
    "vitest": "^0.33.0",
    "vue": "^3.3.4",
    "vue-docgen-api": "^4.73.0",
    "vue-router": "^4.2.4",
    "vue-tsc": "^1.8.5"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "vue-router": {
      "optional": true
    }
  }
}
